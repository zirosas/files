<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
     <!-- App name -->
     <string name="app_name">Cafe Tracker</string>
     <!-- Simple string with placeholder -->
     <string name="welcome_message">Welcome, %1$s!</string>
    <!-- Literal percent (disable formatting to avoid placeholder parsing) -->
    <string name="literal_percent" formatted="false">Save 100% today</string>
    <!-- Non-translatable config -->
    <string name="api_base_url" translatable="false">https://api.example.com</string>
    <!-- Plurals -->
    <plurals name="items_in_cart">
        <item quantity="one">%1$d item in your cart</item>
        <item quantity="other">%1$d items in your cart</item>
    </plurals>
    <!-- String array -->
    <string-array name="coffee_sizes">
        <item>Small</item>
        <item>Medium</item>
        <item>Large</item>
    </string-array>
    <!-- Rich text (HTML via CDATA); render with fromHtml or setText w/ links -->
    <string name="terms_html"><![CDATA[
        By using this app you agree to the <b>Terms</b> and
        <a href="https://example.com/privacy">Privacy Policy</a>.
    ]]></string>
    <!-- Example with xliff placeholder labeling -->
    <string name="greeting_with_city">
        Welcome to <xliff:g id="city_name">%1$s</xliff:g>
    </string>
    <!-- Escaping special characters -->
    <string name="brand_tagline">Fresh &amp; fast coffee</string>
    <string name="quote_example">He said, "Hello!"</string>
</resources>